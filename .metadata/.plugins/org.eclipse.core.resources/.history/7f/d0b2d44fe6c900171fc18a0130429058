
public class Ques7 {
	
	
	public static void main (String[] args) {
		
		final int ROWS = 3;
		final int COLS = 3;
		int[][] board = new int[ROWS][COLS];
		int i;
		int j;
		int value = 1;
		for(i=0; i < board.length; i++) {
			for(j=0; j < board[i].length; j++) {
				board[i][j] = value;				
								
			}
			System.out.println(board[i][j]);
			value++;
		}
		
	}

}

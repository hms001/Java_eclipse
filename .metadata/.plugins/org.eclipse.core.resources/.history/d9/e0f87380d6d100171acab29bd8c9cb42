package lab2;

public class Student {
	
	private static int id = 12;
	private String name;
	private String SSN;
	private int userID;

	public Student(String name,String SSN) {
		
		System.out.println("E-mail - " + name + "@email.com");
	}
	
	public int uID() {
		userID = id + (int)(Math.random()*9000)+1000 + Integer.parseInt(name);
		return userID;
	}

}

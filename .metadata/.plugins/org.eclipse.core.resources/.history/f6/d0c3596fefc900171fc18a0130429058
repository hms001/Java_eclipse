
public class Ques7 {
	
	
	public static void main (String[] args) {
		
		int board[][] = {{1,2,3},{4,5,6},{7,8,9}};
		display(board);
	}
	public static void display(int num[][]) {
		
		for(int i=0; i < num.length; i++) {
			for(int j=0; j < num[i].length; j++) {
				System.out.print(num[i][j]+"\t");				
								
			}
			System.out.println();
		}
		
	}

}

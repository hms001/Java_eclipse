
public class Ques7 {
	
	
	public static void main (String[] args) {
		
		final int ROWS = 3;
		final int COLS = 3;
		int[][] board = new int[ROWS][COLS];
		int i;
		for(i=0; i < board.length; i++) {
			for(int j=0; j < board[i].length; j++) {
				int value = 1;
				board[i][j] = value++;				
				System.out.println(board[i][j]);
				
			}
		}
		
	}

}
